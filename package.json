{
  "name": "reablocks-website",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.1.1",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "copy:blocks": "cpx \"node_modules/reablocks/dist/blocks/*.tsx\" \"src/stories/blocks\" -C",
    "copy:stories": "cpx \"node_modules/reablocks/dist/stories/*.tsx\" \"src/stories/components\" -C",
    "copy:styles": "cpx \"node_modules/reablocks/dist/index.css\" \"public/reablocks\"",
    "postinstall": "pnpm run copy:blocks && pnpm run copy:stories && pnpm run copy:styles",
    "postbuild": "pagefind --site .next/server/app --output-path public/_pagefind",
    "lint": "next lint"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@storybook/preview-api": "^8.6.14",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "motion": "^12.5.0",
    "next": "^15.3.3",
    "nextra": "^4.2.17",
    "posthog-js": "^1.131.4",
    "reablocks": "9.1.3",
    "reablocks-docs-theme": "^2.1.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.60.0",
    "react-live-runner": "^1.0.7",
    "react-shadow": "^20.6.0",
    "react-syntax-highlighter": "^15.5.0",
    "sharp": "^0.33.3",
    "tailwind-merge": "^2.2.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-syntax-highlighter": "^15.5.13",
    "cpx": "^1.5.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.1",
    "pagefind": "^1.3.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "typescript": "^5"
  }
}
